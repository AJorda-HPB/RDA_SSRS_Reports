    <DataSet Name="DropdownDates">
      <Query>
        <DataSourceName>SAGE_RR</DataSourceName>
        <CommandText>
;with cte as(
    select 1[n]
        ,dateadd(month,datediff(month,0,getdate())-1,0)StartDate 
        ,dateadd(DD,-1,dateadd(month,datediff(month,0,getdate()),0))EndDate
    union all 
    select n+1
        ,dateadd(month,datediff(month,0,StartDate)-1,0)
        ,dateadd(DD,-1,dateadd(month,datediff(month,0,StartDate),0))
    from cte 
    where StartDate &gt; datefromparts(year(getdate())-3,1,1)
)
select StartDate[Value_StartDate]
    ,format(StartDate,'MM/dd/yyyy')Label_StartDate
    ,EndDate[Value_EndDate]
    ,format(EndDate,'MM/dd/yyyy')Label_EndDate
from cte 
order by 1 desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Value_StartDate">
          <DataField>Value_StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Label_StartDate">
          <DataField>Label_StartDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value_EndDate">
          <DataField>Value_EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Label_EndDate">
          <DataField>Label_EndDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
